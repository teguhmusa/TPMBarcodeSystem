<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teguhmusaharpa.tpmbarcodesystem">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MasterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateLocationActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".CaptureActivity" />
        <activity android:name=".EmployeeActivity" />
        <activity android:name=".InventoryActivity" />
        <activity android:name=".ShowInventoryActivity" />
        <activity android:name=".DetailMesinActivity"></activity>
    </application>

</manifest>